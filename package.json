{
  "name": "tmpps.boardless.web",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "electron-main.js",
  "dependencies": {
    "@material-ui/core": "^3.1.0",
    "@material-ui/icons": "^3.0.1",
    "inversify": "^4.13.0",
    "query-string": "^6.2.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-jss": "^8.6.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "redux": "^4.0.0",
    "redux-localstorage": "^0.4.1",
    "reflect-metadata": "^0.1.12",
    "typeface-roboto": "0.0.54",
    "typescript-fsa": "^3.0.0-beta-2",
    "typescript-fsa-reducers": "^0.4.5",
    "url-join": "^4.0.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "test-ci": "CI=true react-scripts-ts test --env=jsdom",
    "mock": "node .mock/server.js",
    "electron": "run-s electron-cleanup build electron-move electron-package electron-cleanup-temp",
    "electron-cleanup": "rimraf ./dist",
    "electron-cleanup-temp": "rimraf ./dist/temp",
    "electron-move": "run-p electron-move:*",
    "electron-move:web": "cpx \"./build/**/*\" ./dist/temp/build",
    "electron-move:main": "cpx \"./electron-main.js\" ./dist/temp",
    "electron-move:package": "cpx \"./package.json\" ./dist/temp",
    "electron-package": "electron-packager ./dist/temp --platform=all --arch=all --out=dist --asar --overwrite --icon=\"./public/favicon.ico\" "
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "@types/query-string": "^6.1.1",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/url-join": "^0.8.2",
    "cpx": "^1.5.0",
    "electron": "^3.0.6",
    "electron-packager": "^12.2.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "json-server": "^0.14.0",
    "npm-run-all": "^4.1.3",
    "react-test-renderer": "^16.5.2",
    "rimraf": "^2.6.2",
    "typescript": "^3.1.0-dev.20180922"
  },
  "jest": {
    "moduleNameMapper": {
      "^src/(.+)": "<rootDir>/src/$1"
    }
  }
}
